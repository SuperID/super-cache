{
  "name": "super-cache",
  "version": "0.0.13",
  "description": "Flexible cache manager for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "mocha -t 30000",
    "coverage2": "istanbul cover _mocha -- -t 30000",
    "coverage": "istanbul cover _mocha --report lcovonly -- -t 30000 -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prepublish": "npm run coverage && ./build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperID/super-cache.git"
  },
  "keywords": [
    "cache"
  ],
  "author": "Zongmin Lei <leizongmin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SuperID/super-cache/issues"
  },
  "homepage": "https://github.com/SuperID/super-cache",
  "dependencies": {
    "debug": "^2.1.3",
    "has-localstorage": "^1.0.1",
    "ioredis": "^1.15.1",
    "memjs": "^0.8.9",
    "node-localstorage": "^0.5.2"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "browserify": "^13.0.0",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "should": "^8.2.2",
    "uglify-js": "^2.6.2"
  }
}
